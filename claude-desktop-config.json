{
  "mcpServers": {
    "tides": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--name", "tides-mcp-session",
        "-e", "ANTHROPIC_API_KEY",
        "-v", "${HOME}/Documents/TideReports:/app/reports",
        "--cpus=1",
        "--memory=2g",
        "mcp-fleet:tides"
      ]
    },
    "compass": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--name", "compass-mcp-session",
        "-v", "${HOME}/Documents/CompassProjects:/app/workspace",
        "--cpus=1",
        "--memory=2g",
        "mcp-fleet:compass"
      ]
    },
    "of": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--name", "of-mcp-session",
        "-e", "ANTHROPIC_API_KEY",
        "-v", "${HOME}/Documents/OrchestrationProjects:/app/projects",
        "--cpus=1",
        "--memory=2g",
        "mcp-fleet:of"
      ]
    }
  }
}