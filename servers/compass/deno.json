{
  "name": "@servers/compass",
  "version": "1.0.0",
  "exports": {
    ".": "./mcp_main.ts"
  },
  "imports": {
    "@packages/": "../../packages/",
    "@std/": "https://deno.land/std@0.208.0/",
    "zod": "npm:zod@^3.22.0"
  },
  "tasks": {
    "start": "deno run --allow-all mcp_main.ts",
    "test": "deno test --allow-all tests/",
    "test:unit": "deno test --allow-all tests/unit/",
    "test:integration": "deno test --allow-all tests/integration/",
    "test:e2e": "deno test --allow-all tests/e2e/",
    "test:watch": "deno test --allow-all --watch tests/",
    "dev": "deno run --allow-all --watch mcp_main.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check mcp_main.ts"
  }
}