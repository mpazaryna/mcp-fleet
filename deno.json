{
  "workspace": [
    "./servers/orchestration-framework",
    "./servers/compass",
    "./servers/tides",
    "./servers/toolkit",
    "./packages/mcp-core",
    "./packages/claude-client",
    "./packages/common-tools"
  ],
  "imports": {
    "@packages/": "./packages/",
    "@servers/": "./servers/",
    "@std/": "https://deno.land/std@0.208.0/",
    "@mizchi/mcp-helper": "jsr:@mizchi/mcp-helper@^0.0.4",
    "zod": "npm:zod@^3.22.0",
    "@anthropic-ai/sdk": "npm:@anthropic-ai/sdk@^0.24.0",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^0.5.0"
  },
  "tasks": {
    "test": "deno task test:packages && deno task test:servers",
    "test:packages": "deno test packages/",
    "test:servers": "deno test servers/",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "check": "deno check servers/*/mcp_main.ts",
    "docker:build": "docker build -t mcp-fleet:base .",
    "docker:build:orchestration": "docker build -t mcp-fleet:orchestration-framework -f Dockerfile.orchestration .",
    "docker:build:tides": "docker build -t mcp-fleet:tides -f Dockerfile.tides .",
    "docker:build:toolkit": "docker build -t mcp-fleet:toolkit -f Dockerfile.toolkit .",
    "docker:build:all": "deno task docker:build && deno task docker:build:orchestration && deno task docker:build:tides && deno task docker:build:toolkit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f"
  }
}