{
  "workspace": [
    "./servers/orchestration-framework",
    "./servers/compass",
    "./servers/tides",
    "./servers/toolkit",
    "./packages/mcp-core",
    "./packages/claude-client",
    "./packages/common-tools"
  ],
  "imports": {
    "@packages/": "./packages/",
    "@servers/": "./servers/",
    "@std/": "https://deno.land/std@0.208.0/",
    "@mizchi/mcp-helper": "jsr:@mizchi/mcp-helper@^0.0.4",
    "zod": "npm:zod@^3.22.0",
    "@anthropic-ai/sdk": "npm:@anthropic-ai/sdk@^0.24.0",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^0.5.0"
  },
  "tasks": {
    "test": "uv run pytest tests/ -v",
    "test:packages": "uv run pytest tests/packages/ -v",
    "test:servers": "uv run pytest tests/servers/ -v",
    "lint": "uv run ruff check .",
    "fmt": "uv run black .",
    "typecheck": "uv run mypy python-servers/ python-packages/",
    "docker:build:python-base": "docker build -t mcp-fleet:python-base -f Dockerfile.python-base .",
    "docker:build:tides": "docker build -t mcp-fleet:tides -f Dockerfile.tides .",
    "docker:build:compass": "docker build -t mcp-fleet:compass -f Dockerfile.compass .",
    "docker:build:toolkit": "docker build -t mcp-fleet:toolkit -f Dockerfile.toolkit .",
    "docker:build:of": "docker build -t mcp-fleet:of -f Dockerfile.of .",
    "docker:build:all": "deno task docker:build:python-base && deno task docker:build:tides && deno task docker:build:compass && deno task docker:build:toolkit && deno task docker:build:of",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:publish": "./scripts/docker-hub-publish.sh",
    "docker:login": "docker login",
    "config:docker": "cp claude-desktop-config.json claude-desktop-config-active.json",
    "config:local": "cp claude-desktop-config-local.json claude-desktop-config-active.json",
    "config:show": "cat claude-desktop-config-active.json"
  }
}